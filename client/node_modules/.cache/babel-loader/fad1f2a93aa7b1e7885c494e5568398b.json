 '@/services/SubvuesService'\r\n\r\nexport default {\r\n    name: 'create-post',\r\n\r\n    data() {\r\n        return {\r\n            name: '',\r\n            description: '',\r\n            moderators: '',\r\n            error: null\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        permalink() {\r\n            if (this.name) {\r\n                return location.protocol + '//' + location.hostname + (location.port ? ':' + location.port : '') + '/#/s/' + this.name.toLowerCase()\r\n                    .replace(new RegExp(' ', 'g'), '-')\r\n                    .replace(new RegExp('\\\\.', 'g'), '')\r\n                    .replace(new RegExp(',', 'g'), '')\r\n                    .replace(new RegExp('!', 'g'), '')\r\n                    .replace(new RegExp('\\\\?', 'g'), '')\r\n            } else {\r\n                return null\r\n            }\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        create() {\r\n            SubvuesService.create({\r\n                name: this.name,\r\n                description: this.description,\r\n                moderators: this.moderators\r\n            })\r\n                .then(response => {\r\n                    this.$router.push({\r\n                        name: 'Subvue',\r\n                        params: { name: response.data.permalink }\r\n                    })\r\n                })\r\n                .catch(e => {\r\n                    this.error = e.response.data.error\r\n                })\r\n        }\r\n    },\r\n\r\n    mounted() {\r\n        // Focus name\r\n        this.$refs.name.focus();\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"css\">\r\nform {\r\n    max-width: 1500px;\r\n    width: 100%;\r\n    margin: 0 auto;\r\n}\r\n\r\ninput,\r\ntextarea {\r\n    display: block;\r\n    width: 100%;\r\n    margin: 0;\r\n    border: none;\r\n    background: rgb(223, 224, 221);\r\n    padding: 15px;\r\n    margin: 20px 0;\r\n\r\n    -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */\r\n    -moz-box-sizing: border-box; /* Firefox, other Gecko */\r\n    box-sizing: border-box; /* Opera/IE 8+ */\r\n}\r\n\r\n.permalink {\r\n    color: gray;\r\n    text-align: right;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}